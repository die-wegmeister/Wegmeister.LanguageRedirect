{
    "name": "wegmeister/language-redirect",
    "description": "A Neos CMS package to handle language redirects based on the browser language.",
    "type": "neos-plugin",
    "license": [
        "GPL-3.0-or-later"
    ],
    "require": {
        "neos/neos": ">= 7.0.0",
        "php": ">= 8.0"
    },
    "autoload": {
        "psr-4": {
            "Wegmeister\\LanguageRedirect\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Wegmeister.LanguageRedirect"
        }
    }
}
